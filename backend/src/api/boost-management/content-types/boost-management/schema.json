{
  "kind": "collectionType",
  "collectionName": "boost_managements",
  "info": {
    "singularName": "boost-management",
    "pluralName": "boost-managements",
    "displayName": "Boost Management Subscription",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "nickname": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "phone_number": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true,
      "private": true
    },
    "current_boosts": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Standard",
        "Premium",
        "Ludicrous"
      ],
      "default": "Standard"
    },
    "upcoming_boosts": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Standard",
        "Premium",
        "Ludicrous"
      ],
      "default": "Standard"
    },
    "premium_balance": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "required": true,
      "max": 500,
      "min": 0
    },
    "ludicrous_balance": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "max": 500,
      "min": 0,
      "required": true
    },
    "owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ton-user-management.ton-user-management"
    },
    "boost_now": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "donut.boost"
    },
    "schedule_boost": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "donut.schedule-boost"
    }
  }
}
